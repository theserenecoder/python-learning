     "execution_count": 47,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "#original size is 6 and newsize is 12 therefore elements are repeated\n",
    "print(a)\n",
    "np.resize(a, (3,4))"
